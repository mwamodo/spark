{
  "name": "mwamodo/spark",
  "description": "Spark provides a starter scaffolding for Laravel SaaS applications.",
  "keywords": [
    "laravel",
    "stripe",
    "billing",
    "scaffolding"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Erick Mwamodo",
      "email": "erick@mwamodo.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "laravel/cashier": "^9.0.1",
    "erusev/parsedown": "~1.0",
    "guzzlehttp/guzzle": "~6.0",
    "dompdf/dompdf": "^0.8.2"
  },
  "autoload": {
    "psr-4": {
      "Laravel\\Spark\\": "app/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "dev",
  "require-dev": {
    "composer/composer": "^2.0@dev"
  }
}
